{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "webview:default",
    "app:default",
    "resources:default",
    "image:default",
    "menu:default",
    "tray:default",
    "store:allow-get",
    "store:allow-set",
    "store:allow-save",
    "store:allow-load"
  ]
}
